{
  "name": "informaticauco/simplesamlphp-module-ucofilter",
  "description": "A SimpleSAMLphp module to filter our own attributes",
  "license": "MIT",
  "type": "simplesamlphp-module",
  "autoload": {
    "psr-0": {
      "SimpleSAML\\Modules\\UcoFilter\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SimpleSAML\\Modules\\UcoFilter\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "simplesamlphp/composer-module-installer": "^1.0",
    "symfony/expression-language": "^2.8|^3.0|^4.0",
    "webmozart/assert": "^1.3"
  },
  "require-dev": {
    "simplesamlphp/simplesamlphp": "^1.15",
    "phpunit/phpunit": "^7.4",
    "friendsofphp/php-cs-fixer": "^2.13",
    "roave/security-advisories": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
